<?php

/**
 * @file
 * Adds discussion groups to Drupal.
 */

/**
 * Implements hook_theme().
 */
function discussions_theme() {
  $theme = [];

  $theme['discussions_discussion_add_list'] = array(
    'render element' => 'content',
    'variables' => array('content' => NULL),
    'file' => 'discussions.page.inc',
  );

  return $theme;
}
